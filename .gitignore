################################################################################
# .gitignore for Visual Studio & .NET projects
################################################################################

# User-specific files
*.user

# Visual Studio folders
.vs/
.vscode/

# Build output
**/bin/
**/obj/

# NuGet packages
**/packages/

# Project-specific files
Api/Api.csproj.user

# Ignore all test results
TestResults/

# Ignore all logs
*.log

# Ignore all generated files
*.generated.*

# SQLite database files
*.db-shm
*.db-wal